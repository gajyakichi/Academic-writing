% 参考文献データベース (BibTeX形式)
% 論文のタイプ別にエントリーを追加してください

% ===== 学術論文の例 =====
@article{example_article,
  author    = {著者名, 太郎 and 共著者名, 花子},
  title     = {論文タイトルの例},
  journal   = {ジャーナル名},
  year      = {2024},
  volume    = {1},
  number    = {1},
  pages     = {1--10},
  doi       = {10.1234/example.doi}
}

% ===== 書籍の例 =====
@book{example_book,
  author    = {著者名, 太郎},
  title     = {書籍タイトルの例},
  publisher = {出版社名},
  year      = {2023},
  address   = {東京},
  edition   = {第2版}
}

% ===== 書籍の章の例 =====
@incollection{example_chapter,
  author    = {著者名, 花子},
  title     = {章のタイトル},
  booktitle = {書籍全体のタイトル},
  publisher = {出版社名},
  year      = {2023},
  pages     = {50--75},
  editor    = {編集者名, 一郎}
}

% ===== 学会発表の例 =====
@inproceedings{example_conference,
  author    = {著者名, 太郎},
  title     = {発表タイトル},
  booktitle = {学会名},
  year      = {2024},
  pages     = {100--105},
  address   = {大阪}
}

% ===== Webサイトの例 =====
@misc{example_website,
  author    = {組織名},
  title     = {Webページのタイトル},
  year      = {2024},
  url       = {https://example.com},
  note      = {アクセス日: 2024年1月1日}
}

